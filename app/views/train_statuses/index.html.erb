<p id="notice"><%= notice %></p>

<h1>L Train Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @train_statuses.each do |train_status| %>
      <tr>
        <td><%= train_status.message %></td>
        <td><%= train_status.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= button_to "Get the latest status!", action: 'create'%>
